<resources  xmlns:app="http://schemas.android.com/tools">

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/ColorPrincipal</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:statusBarColor">@color/ColorAzulStatus</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="GetUp" parent="Theme.AppCompat.NoActionBar" >
        <item name="colorPrimary">@color/ColorPrincipalGetup</item>
        <item name="colorPrimaryDark">@color/ColorBarraStatusAzul</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:statusBarColor">@color/ColorAzulStatus</item>
        <item name="android:textColorPrimary">@android:color/black</item>
        <item name="android:textColorSecondaryInverse">@android:color/holo_green_light</item>
        <item name="android:colorBackground">@android:color/black</item>
    </style>

    <style name="Theme">aas</style>


</resources>
