<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#8fd2ca"
    tools:context=".Activitys.GastoTiempo">

    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="120dp"
        android:layout_height="100dp"
        android:layout_above="@+id/textView17"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="18dp"
        android:layout_marginStart="25dp"
        app:srcCompat="@drawable/breakfast" />

    <ImageView
        android:id="@+id/imageView17"
        android:layout_width="120dp"
        android:layout_height="128dp"
        android:layout_above="@+id/imageView15"
        android:layout_alignStart="@+id/imageView15"
        app:srcCompat="@drawable/shower" />

    <ImageView
        android:id="@+id/imageView18"
        android:layout_width="120dp"
        android:layout_height="110dp"
        android:layout_alignEnd="@+id/textView17"
        android:layout_below="@+id/imageView21"
        android:layout_marginTop="12dp"
        app:srcCompat="@drawable/getup" />

    <ImageView
        android:id="@+id/imageView21"
        android:layout_width="120dp"
        android:layout_height="100dp"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/imageView18"
